{
  "name": "kagwave.com-backend",
  "version": "3.0.0",
  "description": "kagwave.com backend",
  "scripts": {
    "start": "node ",
    "server": "HTTPS=true SSL_CRT_FILE=./.cert/cert.pem SSL_KEY_FILE=./.cert/key.pem nodemon src/index.ts",
    "client": "npm run front --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run start --prefix client\" ",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "postinstall": "npm rebuild node-sass"
  },
